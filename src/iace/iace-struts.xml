	<package name="Patent" namespace="/patent" extends="struts-default">
		<action name="init" class="iace.action.PatentAction" method="init">
			<result>/WEB-INF/jsp/backend/patent/index.jsp</result>
		</action>	
		<action name="index" class="iace.action.PatentAction" method="index">
			<result name="success">/WEB-INF/jsp/backend/patent/index.jsp</result>
			<result name="input">/WEB-INF/jsp/backend/patent/index.jsp</result>
		</action>
		<action name="showDetail" class="iace.action.PatentAction" method="showDetail">
			<result name="success">/WEB-INF/jsp/backend/patent/detail.jsp</result>
			<result name="input">/WEB-INF/jsp/backend/patent/detail.jsp</result>
			<result name="error">/WEB-INF/jsp/backend/patent/index.jsp</result>		
		</action>
		<action name="create" class="iace.action.PatentAction" method="create">
			<result name="success">/WEB-INF/jsp/backend/patent/create.jsp</result>	
		</action>
		<action name="createSubmit" class="iace.action.PatentAction" method="createSubmit">
			<interceptor-ref name="fileUpload">
				<param name="allowedTypes">
					image/png,image/gif,image/jpeg
				</param>
			</interceptor-ref>
			<interceptor-ref name="defaultStack"></interceptor-ref>

			<result name="input">/WEB-INF/jsp/backend/patent/create.jsp</result>
			<result name="success">/WEB-INF/jsp/backend/patent/detail.jsp</result>
		</action>
		<action name="update" class="iace.action.PatentAction" method="update">
			<result name="input">/WEB-INF/jsp/backend/patent/index.jsp</result>
			<result name="success">/WEB-INF/jsp/backend/patent/update.jsp</result>		
		</action>
		<action name="updateSubmit" class="iace.action.PatentAction" method="updateSubmit">
			<interceptor-ref name="fileUpload">
				<param name="allowedTypes">
					image/png,image/gif,image/jpeg
				</param>
			</interceptor-ref>
			<interceptor-ref name="defaultStack"></interceptor-ref>
			
			<result name="input">/WEB-INF/jsp/backend/patent/update.jsp</result>
