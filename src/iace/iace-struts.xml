		</action>
		<action name="showDetail" class="iace.action.ResearchPlanAction" method="showDetail">
			<result name="input">/WEB-INF/jsp/iace/researchPlan/index.jsp</result>
			<result name="success">/WEB-INF/jsp/iace/researchPlan/detail.jsp</result>		
		</action>
		<action name="create" class="iace.action.ResearchPlanAction" method="create">
			<result name="success">/WEB-INF/jsp/iace/researchPlan/create.jsp</result>		
		</action>
		<action name="createSubmit" class="iace.action.ResearchPlanAction" method="createSubmit">		
			<result name="input">/WEB-INF/jsp/iace/researchPlan/create.jsp</result>
			<result name="success">/WEB-INF/jsp/iace/researchPlan/update.jsp</result>
		</action>				
		<action name="update" class="iace.action.ResearchPlanAction" method="update">
			<result name="input">/WEB-INF/jsp/iace/researchPlan/index.jsp</result>
			<result name="success">/WEB-INF/jsp/iace/researchPlan/update.jsp</result>		
		</action>
		<action name="updateSubmit" class="iace.action.ResearchPlanAction" method="updateSubmit">		
			<result name="input">/WEB-INF/jsp/iace/researchPlan/update.jsp</result>
			<result name="success">/WEB-INF/jsp/iace/researchPlan/index.jsp</result>
		</action>
		<action name="delete" class="iace.action.ResearchPlanAction" method="delete">
			<result name="input">/WEB-INF/jsp/iace/researchPlan/index.jsp</result>
			<result name="success">/WEB-INF/jsp/iace/researchPlan/delete.jsp</result>		
		</action>	
		<action name="deleteSubmit" class="iace.action.ResearchPlanAction" method="deleteSubmit">		
			<result name="input">/WEB-INF/jsp/iace/researchPlan/delete.jsp</result>
			<result name="success">/WEB-INF/jsp/iace/researchPlan/index.jsp</result>
		</action>				
		<action name="showTechnologyDetail" class="iace.action.ResearchPlanAction" method="showTechnologyDetail">
			<result name="input">/WEB-INF/jsp/iace/researchPlan/update.jsp</result>
			<result name="success">/WEB-INF/jsp/iace/researchPlan/detailTechnology.jsp</result>		
		</action>
		<action name="createTechnology" class="iace.action.ResearchPlanAction" method="createTechnology">
			<result name="input">/WEB-INF/jsp/iace/researchPlan/update.jsp</result>
			<result name="success">/WEB-INF/jsp/iace/researchPlan/createTechnology.jsp</result>		
		</action>
		<action name="createTechnologySubmit" class="iace.action.ResearchPlanAction" method="createTechnologySubmit">		
			<result name="input">/WEB-INF/jsp/iace/researchPlan/createTechnology.jsp</result>
			<result name="success">/WEB-INF/jsp/iace/researchPlan/update.jsp</result>
		</action>
		<action name="updateTechnology" class="iace.action.ResearchPlanAction" method="updateTechnology">
			<result name="input">/WEB-INF/jsp/iace/researchPlan/update.jsp</result>
			<result name="success">/WEB-INF/jsp/iace/researchPlan/updateTechnology.jsp</result>		
		</action>
		<action name="updateTechnologySubmit" class="iace.action.ResearchPlanAction" method="updateTechnologySubmit">		
			<result name="input">/WEB-INF/jsp/iace/researchPlan/updateTechnology.jsp</result>
			<result name="success">/WEB-INF/jsp/iace/researchPlan/update.jsp</result>
		</action>				
		<action name="deleteTechnology" class="iace.action.ResearchPlanAction" method="deleteTechnology">
			<result name="input">/WEB-INF/jsp/iace/researchPlan/update.jsp</result>
			<result name="success">/WEB-INF/jsp/iace/researchPlan/deleteTechnology.jsp</result>		
		</action>
		<action name="deleteTechnologySubmit" class="iace.action.ResearchPlanAction" method="deleteTechnologySubmit">
			<result name="input">/WEB-INF/jsp/iace/researchPlan/deleteTechnology.jsp</result>
			<result name="success">/WEB-INF/jsp/iace/researchPlan/update.jsp</result>
		</action>
	</package>
	
	<package name="Patent" namespace="/iace/patent" extends="struts-default">
		<action name="init" class="iace.action.PatentAction" method="init">
			<result>/WEB-INF/jsp/iace/patent/index.jsp</result>
		</action>	
		<action name="index" class="iace.action.PatentAction" method="index">
			<result name="success">/WEB-INF/jsp/iace/patent/index.jsp</result>
			<result name="input">/WEB-INF/jsp/iace/patent/index.jsp</result>
		</action>
		<action name="showDetail" class="iace.action.PatentAction" method="showDetail">
			<result name="success">/WEB-INF/jsp/iace/patent/detail.jsp</result>
			<result name="input">/WEB-INF/jsp/iace/patent/detail.jsp</result>			
			<result name="error" type="redirectAction">
				<param name="actionName">index</param>
			</result>			
		</action>
		<action name="create" class="iace.action.PatentAction" method="create">
			<result name="success">/WEB-INF/jsp/iace/patent/create.jsp</result>	
		</action>
		<action name="createSubmit" class="iace.action.PatentAction" method="createSubmit">
			<interceptor-ref name="fileUpload">
				<param name="allowedTypes">
					image/png,image/gif,image/jpeg
				</param>
			</interceptor-ref>
			<interceptor-ref name="defaultStack"></interceptor-ref>

			<result name="input">/WEB-INF/jsp/iace/patent/create.jsp</result>
			<result name="success">/WEB-INF/jsp/iace/patent/detail.jsp</result>
		</action>
		<action name="update" class="iace.action.PatentAction" method="update">
			<result name="input">/WEB-INF/jsp/iace/patent/index.jsp</result>
			<result name="success">/WEB-INF/jsp/iace/patent/update.jsp</result>		
		</action>
		<action name="updateSubmit" class="iace.action.PatentAction" method="updateSubmit">
			<interceptor-ref name="fileUpload">
				<param name="allowedTypes">
					image/png,image/gif,image/jpeg
				</param>
			</interceptor-ref>
			<interceptor-ref name="defaultStack"></interceptor-ref>
			
			<result name="input">/WEB-INF/jsp/iace/patent/update.jsp</result>
			<result name="success">/WEB-INF/jsp/iace/patent/detail.jsp</result>
		</action>	
		<action name="delete" class="iace.action.PatentAction" method="delete">
			<result name="input">/WEB-INF/jsp/iace/patent/index.jsp</result>
			<result name="success">/WEB-INF/jsp/iace/patent/delete.jsp</result>		
		</action>
		<action name="deleteSubmit" class="iace.action.PatentAction" method="deleteSubmit">
			<result name="input">/WEB-INF/jsp/iace/patent/delete.jsp</result>
			<result name="success">/WEB-INF/jsp/iace/patent/index.jsp</result>
		</action>			
	</package>
	
