			<result name="error">/WEB-INF/jsp/backend/option/school/index.jsp</result>
		</action>
		<action name="batchImport" class="iace.action.OptionSchoolAction" method="batchImport">
		    <interceptor-ref name="fileUpload">                
                <param name="allowedTypes">application/ms-excel,application/vnd.ms-excel,application/vnd.openxmlformats-officedocument.spreadsheetml.sheet</param>
            </interceptor-ref> 
            <interceptor-ref name="defaultStack"></interceptor-ref>
		    
		    <result name="success">/WEB-INF/jsp/backend/option/school/index.jsp</result>
		    <result name="input">/WEB-INF/jsp/backend/option/school/create.jsp</result>
		</action>
		<action name="downloadBatchSample" class="iace.action.OptionSchoolAction" method="downloadBatchSample">
			<result name="success" type="stream">
				<param name="contentType">application/octet-stream</param>
				<param name="inputName">sampleFileInputStream</param>
				<param name="contentDisposition">attachment;filename="${downloadFileName}"</param>
				<param name="bufferSize">1024</param>
			</result>	
		</action>		
	</package>	
</struts>
