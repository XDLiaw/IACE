	<queryString>
		<![CDATA[SELECT ei.NAME, ei.COMPANY_ID, ei.MAIN_PRODUCT, ei.PHASE2, ei.PHASE3, ei.ADDRESS,
ei.PERSON_IN_CHARGE_NAME, ei.PERSON_IN_CHARGE_JOBTITLE, ei.PERSON_IN_CHARGE_PHONE, ei.PERSON_IN_CHARGE_EMAIL,
ei.INTERVIEWEE_NAME, ei.INTERVIEWEE_JOBTITLE, ei.INTERVIEWEE_PHONE, ei.INTERVIEWEE_EMAIL,

ert_od.NAME AS RT_phASe1, ert.PHASE2 AS RT_PHASE2, ert.PHASE3 AS RT_PHASE3,
ert.INQUIRED_ORG AS INQUIRED_ORG, ert.INTERVIEW_DATE, ert.RECORD_BY,

es_ohts.NAME AS HAD_TEC_SRC, es.HAD_TECH_SRC_RATION,
es.HAS_COM_COOP_EXP, es.COOP_TOPIC, es.COOP_PROS, es.COOP_CONS,
es.HAS_ACADEMIA_COOP_EXP, es.ACADEMIA_TOPIC, es.ACADEMIA_INTENTION, es.ACADEMIA_PROS, es.ACADEMIA_CONS,
es_ocm.NAME as COOP_MODE, es.SPECIFIC_TOPIC, es.OTHER_COOP_TARGET,

eac.COOP_SCHOOL, eac.CURRENT_COOP_PROJECT_TOPIC, eac.WANTED_COOP_SCHOOL_TOPIC, eac.SUGGESTION

FROM ENTERPRISE_INFO ei
LEFT JOIN OPT_COM_LOCATION ocl on ocl.id = ei.OPT_COM_LOCATION_ID
LEFT JOIN ENTERPRISE_REQUIRE_TECH ert on ert.ENTERPRISE_INFO_ID = ei.ID
LEFT JOIN OPT_DOMAIN ert_od on ert.PHASE1_DOMAIN_ID = ert_od.ID
LEFT JOIN ENTERPRISE_SITUATION es on es.ENTERPRISE_INFO_ID = ei.ID
LEFT JOIN OPT_HAD_TEC_SRC es_ohts on es.OPT_HAD_TEC_SRC_ID = es_ohts.ID
LEFT JOIN OPT_COOPERATE_MODE es_ocm on es.OPT_COOPERATE_MODE_ID = es_ocm.ID
LEFT JOIN ENTERPRISE_ACADEMIA_COOP eac on eac.ENTERPRISE_INFO_ID = ei.ID

where ei.id = $P{enterpriseNeedId}]]>
	</queryString>
